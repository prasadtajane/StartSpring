package com.prasad.learnspring.SpringOne;

public class WindState {
	private int speed = 0;
	private int degree = 0;
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	public int getDegree() {
		return degree;
	}
	public void setDegree(int degree) {
		this.degree = degree;
	}
	
}
